{
  "functions": {
    "source": "functions"
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "database": {
    "rules": "database.rules.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "ai-frontend-main/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      ".firebaserc",
      "database.rules.json",
      "firestore.rules",
      "storage.rules"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true
  },
  "emulators": {
    "apphosting": {
      "port": 5002,
      "rootDirectory": "ai-frontend-main",
      "startCommand": "npm.cmd run dev"
    },
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8081
    },
    "database": {
      "port": 9001
    },
    "hosting": {
      "port": 5000
    },
    "storage": {
      "port": 9199
    },
    "tasks": {
      "port": 9499
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
